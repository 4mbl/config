{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Browser",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "paths": { "*": ["${configDir}/src/*"] },
    "noEmit": true,
    "sourceMap": true
  }
}
